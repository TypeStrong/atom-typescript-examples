{
    "version": "1.5.0-alpha",
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true
    },
    "filesGlob": [
        "./**/*.ts",
        "!./node_modules/**/*.ts"
    ],
    "files": [
        "./awesome/awesome.d.ts",
        "./awesome/js/foo/part.d.ts",
        "./awesome/js/main.d.ts",
        "./awesome/ts/foo/part.ts",
        "./awesome/ts/main.ts",
        "./usage.ts"
    ]
}
